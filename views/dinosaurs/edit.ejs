<form action="/dinosaurs/<%= id %>/?_method=PUT" method="POST">
    <label for="dinosaurName">Name</label>
    <input id="dinosaurName" value="<%= myDino.name %>" type="text" name="name">

    <label for="dinosaurType">Type</label>
    <input id="dinosaurType" value="<%= myDino.type %>" type="text" name="type">

    <input type="submit" value="Update Dino">
</form>